{
  "name": "do-counter-sharding",
  "version": "1.1.1",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "esbuild ./src/index.ts --format=esm --bundle --outdir=./dist && tsc",
    "build-test": "esbuild ./test/index.ts --format=esm --bundle --outdir=./test/dist --out-extension:.js=.mjs",
    "test": "miniflare -w --wrangler-config ./test/wrangler.toml",
    "test-counter": "ts-node ./test/scripts/requests.test.ts",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/g45t345rt/do-counter-sharding"
  },
  "homepage": "https://github.com/g45t345rt/do-counter-sharding#readme",
  "devDependencies": {
    "@cloudflare/workers-types": "^3.3.1",
    "@types/node": "^17.0.13",
    "esbuild": "^0.14.14",
    "inquirer": "^8.2.0",
    "isomorphic-fetch": "^3.0.0",
    "miniflare": "^2.2.0",
    "ts-node": "^10.4.0"
  },
  "dependencies": {
    "itty-router": "^2.4.10",
    "nanoid": "^3.2.0"
  }
}