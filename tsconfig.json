{
  "compilerOptions": {
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "ESNext",
      "WebWorker",
      "DOM"
    ],
    "types": [
      "node",
      "@cloudflare/workers-types"
    ],
  },
  "exclude": [
    "node_modules"
  ]
}
